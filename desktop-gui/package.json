{
  "name": "storeit-desktop-gui",
  "version": "0.0.1",
  "description": "GUI for StoreIt Desktop App",
  "main": "build/electron-main.js",
  "dependencies": {
    "electron-prebuilt": "^1.2.7",
    "jspm": "^0.17.0-beta.22",
    "shelljs": "^0.7.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "coffee-react": "^5.0.0",
    "karma": "^1.1.1"
  },
  "jspm": {
    "name": "storeit-desktop-gui",
    "main": "build/app/index.js",
    "directories": {
      "lib": "build/src",
      "dist": "build/dist",
      "baseURL": ".",
      "packages": "build/jspm_packages"
    },
    "configFiles": {
      "jspm": "jspm.config.js"
    },
    "dependencies": {
      "coffee": "github:forresto/system-coffee@^0.1.2",
      "css": "github:systemjs/plugin-css@^0.1.23",
      "jade": "github:johnsoftek/plugin-jade@^1.1.2"
    },
    "devDependencies": {},
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "http": "github:jspm/nodelibs-http@^0.2.0-alpha",
      "https": "github:jspm/nodelibs-https@^0.2.0-alpha",
      "module": "github:jspm/nodelibs-module@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "tty": "github:jspm/nodelibs-tty@^0.2.0-alpha",
      "url": "github:jspm/nodelibs-url@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha"
    },
    "overrides": {
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      }
    }
  },
  "scripts": {
    "start": "npm run build && npm run gui",
    "test": "karma start karma.config.js",
    "preinstall": "npm prune",
    "postinstall": "jspm install",
    "gui": "electron .",
    "build": "npm run build:lib && coffee -o ./build -c ./electron-main.coffee",
    "build:lib": "babel ../lib/js/src -s -d lib",
    "dev": "npm run build:dev && npm run gui",
    "build:dev": "coffee -w -c electron-main.coffee -o ./build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sevauk/storeit.git"
  },
  "keywords": [
    "StoreIt"
  ],
  "author": "Alexandre Fulgoni",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sevauk/storeit/issues"
  },
  "homepage": "https://github.com/Sevauk/storeit#readme"
}
